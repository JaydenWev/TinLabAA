	<template>
		<name>Queue</name>
		<parameter>chan &amp;exiting</parameter>
		<declaration>//Queue

id_i v;

const int maxWaiting = 4;
const int maxBoatNum = maxBoatId;
int[0, maxBoatNum] boat_id = 1;

id_i queue[maxWaiting];
int[0, maxWaiting] queueSize = 0;

bool isFull(){
    if (queueSize == maxWaiting){
        return true;
    }
    else {
        return false;
    }
}
bool isEmpty() {
    if (queueSize == 0){
        return true;
    }
    else {
        return false;
    }
}
void enterQueue() {
    if (!isFull()){
        queue[queueSize] = boat_id;
        queueSize++;
        if (boat_id == maxBoatNum){
            boat_id = 1;
        } else {
            boat_id++;
        }
    }
}
int exitQueue() {
    id_i temp;
    id_i i;
    if (!isEmpty()) {
            temp = queue[0]; // uit de queue stappen
        for (i = 0; i &lt; queueSize-1 ; i++){
            queue[i] = queue[i+1];
        } queueSize--;
        return temp;
    }
    else {
        return 0;
    }
}


